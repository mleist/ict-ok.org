    <z3c:pagelet
        name="edit_content.html"
        for="org.ict_ok.components.ipnet.interfaces.IIpNet"
        class=".ipnet.EditContent"
        layer="org.ict_ok.skin.interfaces.IBrowserLayer"
        permission="org.ict_ok.components.ipnet.Edit"
    />

