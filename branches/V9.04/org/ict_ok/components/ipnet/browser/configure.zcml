        for="org.ict_ok.components.ipnet.interfaces.IIpNet"
        class=".ipnet.IpNetDetails"
        attribute="actions"
        permission="org.ict_ok.components.ipnet.View"
        layer="org.ict_ok.skin.interfaces.IBrowserLayer"
    />

    <page
